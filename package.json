{
  "name": "genaratetypeschema",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "tsc",
    "build": "parcel build src/index.ts",
    "watch": "parcel watch src/index.ts",
    "generate": "run-s generate:jsonSchema generate:type",
    "generate:jsonSchema": "ts-node ./src/jsonSchema/compile.ts",
    "generate:type": "quicktype --src-lang schema ./out/data.json --lang typescript --out ./out/type.ts --top-level Schema --just-types"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "json-schema-to-typescript": "^13.0.2",
    "quicktype": "^23.0.49",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "ts-node": "^10.9.1"
  }
}
